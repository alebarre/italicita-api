{
  "name": "italicita-backend",
  "version": "1.0.0",
  "description": "Backend para Italicita Delivery",
  "main": "src/app.js",
  "type": "commonjs",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "db:up": "docker-compose up -d",
    "db:down": "docker-compose down",
    "db:logs": "docker-compose logs -f postgres",
    "db:reset": "docker-compose down && docker volume rm italicita-backend_postgres_data && docker-compose up -d && timeout 10 && npm run migrate && npm run seed",
    "db:clean": "docker-compose down && docker volume rm italicita-backend_postgres_data",
    "db:fresh": "npm run db:clean && npm run db:up && timeout 10 && npm run migrate && npm run seed",
    "migrate": "npx knex migrate:latest",
    "migrate:rollback": "npx knex migrate:rollback",
    "seed": "npx knex seed:run",
    "test": "jest"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "knex": "^2.5.1",
    "nodemon": "^3.0.1",
    "pg": "^8.11.3"
  },
  "keywords": [
    "delivery",
    "food",
    "api",
    "express"
  ],
  "author": "Italicita Team",
  "license": "MIT"
}
